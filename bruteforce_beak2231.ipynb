    "def dis_sum ():\n",
    "    N = int(input('분해합 입력 : '))\n",
    "    con = 0\n",
    "    for i in range(N):\n",
    "        temp = i\n",
    "        for j in str(i):\n",
    "            temp += int(j) \n",
    "        if temp == N:\n",
    "            con = i\n",
    "            break\n",
    "    print(con)\n",
    "    return 0\n",
    "\n",
    "dis_sum()\n",
