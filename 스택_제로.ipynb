    "n = int(input())\n",
    "arr = []\n",
    "result = 0\n",
    "for _ in range(n):\n",
    "    k = int(input())\n",
    "    if k == 0:\n",
    "        del arr[-1]\n",
    "        continue\n",
    "    arr.append(k)\n",
    "if len(arr) == 0:\n",
    "    print(0)\n",
    "else:\n",
    "    for i in arr:\n",
    "        result += i\n",
    "    print(result) "
