    "n = int(input())\n",
    "\n",
    "def tile(x):\n",
    "    if x == 0:\n",
    "        return 0\n",
    "    elif x == 1:\n",
    "        return 1\n",
    "    elif x == 2:\n",
    "        return 2\n",
    "    else:\n",
    "        return tile(x-2) + tile(x-1)\n",
    "    \n",
    "print(tile(n)%15746)"
   ]
  }
