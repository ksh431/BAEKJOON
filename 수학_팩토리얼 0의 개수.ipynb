    "n = int(input())\n",
    "\n",
    "def fac(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return fac(n-1)*n\n",
    "\n",
    "count = 0\n",
    "num = fac(n)\n",
    "for _ in range(n):\n",
    "    if num%10 == 0:\n",
    "        count += 1\n",
    "        num = num//10\n",
    "    else:\n",
    "        break\n",
    "print(count)\n",
    "        \n",
    "    "
   ]
  }
