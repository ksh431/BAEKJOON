   "source": [
    "N, K = map(int, input().split())\n",
    "arr = []\n",
    "for _ in range(N):\n",
    "    arr.append(int(input()))\n",
    "\n",
    "result = 0\n",
    "i = 0\n",
    "while K>0:\n",
    "    if arr[i] > K:\n",
    "        result += K//arr[i-1]\n",
    "        K -= (K//arr[i-1])*arr[i-1]\n",
    "        i = -1\n",
    "    i += 1\n",
    "    \n",
    "print(result)"
   ]
  }
