   "source": [
    "def snail():\n",
    "    A, B, V = map(int, input('달팽이 하루 이동 거리, 손실 거리, 막대 높이 : ').split())\n",
    "    total = 0\n",
    "    day = 0\n",
    "    while True:\n",
    "        total += A\n",
    "        day += 1\n",
    "        if total >= V:\n",
    "            break\n",
    "        total -= B\n",
    "    print(day)\n",
    "snail()"
   ]
  }
